@startchen
left to right direction

entity Пациент {
  пациент_id <<key>>
  ФИО
  телефон
  email
  дата_рождения
  created_at
}

entity Врач {
  врач_id <<key>>
  ФИО
  специализация
  кабинет
  телефон
  created_at
}

entity Услуга {
  услуга_id <<key>>
  название
  описание
  длительность_мин
  цена
  доступна
}

entity Запись {
  запись_id <<key>>
  дата_время
  статус
  created_at
  updated_at
  стоимость
}

relationship записывается {
}

relationship принимает {
}

relationship включает {
}

Пациент -1- записывается
записывается -N- Запись

Врач -1- принимает
принимает -N- Запись

Услуга -1- включает
включает -N- Запись

@endchen
